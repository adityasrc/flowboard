generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int @id @default(autoincrement())
  username  String @unique
  name      String
  email     String @unique
  password  String
  rooms     Room[]
}

model Room {
  id        Int @id @default(autoincrement())
  slug      String @unique
  createdAt DateTime
  userId    Int
  user      User @relation(fields: [userId], references: [id])
}
